
{
  "request": {
    "urlPathPattern": "/catalog/.*/rename",
    "method": "POST",
    "headers": {
      "X-Auth-Token": {
        "equalTo": "SESSION_ID_GOES_HERE"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.name =~ /.*/i)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "body": "{\n  \"success\":true   \n}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}