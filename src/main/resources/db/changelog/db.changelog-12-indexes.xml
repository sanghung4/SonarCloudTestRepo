<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <!-- Add proper indexes to bia -->
    <changeSet id="9845871945013745-1" author="erik.gabrielsen@dialexa.com">
        <createIndex indexName="idx_pi_count" tableName="pi_counts">
            <column name="branch_id"/>
            <column name="erp_count_id"/>
            <column name="is_deleted"/>
        </createIndex>
        <createIndex indexName="idx_pi_locations" tableName="pi_locations">
            <column name="erp_location_id"/>
            <column name="branch_id"/>
        </createIndex>
        <createIndex indexName="idx_pi_locations_count" tableName="pi_location_count">
            <column name="count_id"/>
            <column name="location_id"/>
            <column name="committed"/>
        </createIndex>
        <createIndex indexName="idx_pi_location_count_item" tableName="pi_count_location_item">
            <column name="location_count_id"/>
            <column name="status"/>
        </createIndex>
        <createIndex indexName="idx_pi_count_location_item_quantity" tableName="pi_count_location_item_quantity">
            <column name="count_location_item_id"/>
        </createIndex>
        <createIndex indexName="idx_pi_count_location_item_variance_quantity" tableName="pi_count_location_item_variance_quantity">
            <column name="count_location_item_id"/>
        </createIndex>
        <createIndex indexName="idx_pi_location_count_metric" tableName="pi_location_count_metric">
            <column name="location_count_id"/>
        </createIndex>
        <createIndex indexName="idx_pi_write_ins" tableName="pi_write_ins">
            <column name="location_count_id"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>