<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet id="1352654987499-1" author="huy">
          <createTable tableName="metrics_logins">
              <column name="id" type="UUID">
                  <constraints nullable="false" primaryKey="true" primaryKeyName="metrics_loginsPK"/>
              </column>
              <column name="user_email" type="VARCHAR(255)">
                  <constraints nullable="false"></constraints>
              </column>
              <column name="branch_id" type="VARCHAR(255)">
                  <constraints nullable="false"></constraints>
              </column>
              <column name="login_at" type="TIMESTAMP WITHOUT TIMEZONE">
                  <constraints nullable="false"></constraints>
              </column>
          </createTable>
    </changeSet>
    <changeSet id="1548978563523-1" author="huy">
        <createTable tableName="metrics_branch_division">
            <column name="id" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="metrics_branch_divisionPK"/>
            </column>
            <column name="branch_id" type="VARCHAR(255)">
                <constraints unique="true" nullable="false"></constraints>
            </column>
            <column name="branch_name" type="VARCHAR(255)"/>
            <column name="branch_state" type="VARCHAR(255)"/>
            <column name="division_id" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>