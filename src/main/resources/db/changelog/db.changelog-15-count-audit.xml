            </column>
            <column name="erp_count_id" type="VARCHAR(255)">
                <constraints nullable="false" />
            </column>
            <column name="erp_branch_num" type="VARCHAR(255)">
                <constraints nullable="false" />
            </column>
            <column name="created_by" type="VARCHAR(255)">
                <constraints nullable="false" />
            </column>
            <column name="created_at" type="TIMESTAMP WITHOUT TIME ZONE">
                <constraints nullable="false" />
            </column>
            <column name="erp_system" type="VARCHAR(255)">
                <constraints nullable="false" />
            </column>
            <column name="action" type="VARCHAR(255)">
                <constraints nullable="false" />
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
