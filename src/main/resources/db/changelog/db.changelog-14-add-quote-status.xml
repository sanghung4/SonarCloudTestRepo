<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog logicalFilePath="classpath:changelog-14.xml" xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet author="tim@dialexa.com" id="addCreditCardPaymentType">
        <preConditions onFail="MARK_RAN" onFailMessage="payment_method_type_enum does not exist">
            <not>
                <tableExists tableName="quote_status" />
            </not>
        </preConditions>
        <createTable tableName="quote_status">
            <column name="id" type="VARCHAR(24)">
                <constraints nullable="false" primaryKey="true" unique="true" />
            </column>
            <column name="status" type="VARCHAR(24)">
                <constraints nullable="false" />
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="quote_status" />
        </rollback>
    </changeSet>
    <changeSet author="tim@dialexa.com" id="tag-14-add-quote-status">
        <tagDatabase tag="products-v14"/>
    </changeSet>
</databaseChangeLog>
