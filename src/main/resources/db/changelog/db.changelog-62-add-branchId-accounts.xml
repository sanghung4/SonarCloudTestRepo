<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<databaseChangeLog
    logicalFilePath="classpath:changelog-62.xml"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
>
    <changeSet author="sameer.ahmed2@morsco.com" id="addBranchIDToAccounts">
        <preConditions onFail="MARK_RAN" onFailMessage="accounts table already contains branch_id">
            <not>
                <columnExists tableName="accounts" columnName="branch_id" />
            </not>
        </preConditions>
        <addColumn tableName="accounts">
            <column name="branch_id" type="VARCHAR(256)">
                <constraints nullable="true" />
            </column>
        </addColumn>
        <rollback>
            <dropColumn tableName="accounts" columnName="branch_id" />
        </rollback>
    </changeSet>
    <changeSet author="sameer.ahmed2@morsco.com" id="v62-add-branchId-accounts">
        <tagDatabase tag="v62" />
    </changeSet>
</databaseChangeLog>
