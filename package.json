{
  "name": "@dialexa/reece-portal",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": "^16.13.0"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@contentful/rich-text-types": "^15.12.1",
