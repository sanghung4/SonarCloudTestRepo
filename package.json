    "@headlessui/react": "^1.7.7",
    "@hookform/resolvers": "^2.8.1",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.67",
    "@mui/material": "^5.4.0",
    "@okta/okta-auth-js": "^5.0.0",
    "@okta/okta-react": "^6.0.0",
    "@react-google-maps/api": "^2.2.0",
    "apollo-upload-client": "^17.0.0",
    "classnames": "^2.3.2",
    "clsx": "^1.1.1",
    "date-fns": "^2.27.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "graphql": "^16.2.0",
    "i18next": "^21.6.3",
    "kill-port": "^1.6.1",
    "lodash-es": "^4.17.15",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-csv": "^2.2.2",
    "react-day-picker": "^8.0.0-beta.36",
    "react-dom": "^17.0.2",
    "react-dotdotdot": "^1.3.1",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.22.2",
    "react-hotkeys": "^2.0.0",
    "react-i18next": "^11.15.1",
    "react-imask": "^6.4.2",
    "react-router-dom": "^5.3.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^4.0.3",
    "react-slugify": "^2.1.0",
    "react-table": "^7.7.0",
    "react-window": "^1.8.6",
    "sass": "^1.57.1",
    "smoothscroll-polyfill": "^0.4.4",
    "unionize": "^3.1.0",
    "use-resize-observer": "^8.0.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "prestart": "./scripts/gen-env.sh",
    "start:generate": "concurrently -k -p \"[{name}]\" -n \"GraphQL,React\" -c \"magenta.bold,cyan.bold\" \"yarn generate --watch\" \"react-scripts start\"",
    "start:i18n": "REACT_APP_I18N_LNG=test react-scripts start",
    "build": "react-scripts build",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:test-env": "env-cmd -f .env.test-env react-scripts build",
    "build:test": "npx tsc -p tests/tsconfig.test.json",
    "build:uat-env": "env-cmd -f .env.uat-env react-scripts build",
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:snapshot": "yarn test -u",
    "init:project": "./scripts/init-dev.sh",
    "eject": "react-scripts eject",
    "e2e": "yarn build:test && npx percy exec -- wdio tests/dist/wdio.conf.js",
    "lint": "./node_modules/.bin/eslint --ext ts,tsx,js,jsx src/",
    "prettier": "./node_modules/.bin/prettier --check *",
    "prepare": "husky install",
    "sitemap": "ts-node --project tsconfig.node.json ./src/sitemap-generator.ts",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": "*.d.ts",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@percy/cli": "^1.0.0-beta.24",
    "@percy/webdriverio": "^1.0.4",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack4": "^6.5.15",
    "@storybook/manager-webpack4": "^6.5.15",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/chai": "^4.3.0",
    "@types/chance": "^1.1.3",
    "@types/css-mediaquery": "^0.1.1",
    "@types/google.maps": "^3.47.2",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.39",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-table": "^7.7.9",
    "@types/react-window": "^1.8.5",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/yup": "^0.29.13",
    "@wdio/cli": "^6.8.1",
    "@wdio/local-runner": "^6.8.1",
    "@wdio/mocha-framework": "^6.8.0",
    "@wdio/selenium-standalone-service": "^6.8.0",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.8.0",
    "chai": "^4.2.0",
    "chance": "^1.1.8",
    "chromedriver": "^99.0.0",
    "concurrently": "^6.5.1",
    "css-mediaquery": "^0.1.2",
    "husky": ">=6",
    "jest-css-modules-transform": "^4.3.0",
    "jest-junit": "^13.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "react-router-navigation-prompt": "^1.9.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "<4.5.0",
    "wdio-chromedriver-service": "^6.0.4"
  },
  "jest": {
    "transform": {
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform",
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es)"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/generated/**",
      "!src/icons/**",
      "!src/test-utils/**",
      "!src/**/*.stories.tsx"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9",
    "babel-loader": "8.1.0"
  }
}
